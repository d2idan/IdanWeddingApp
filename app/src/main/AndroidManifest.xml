<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.chenandidanwedding">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".StuffActivity"
            android:label="אטרקציות"
            android:theme="@style/StuffTheme"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ChenActivity"
            android:label="חן"
            android:theme="@style/ChenTheme"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".IdanActivity"
            android:label="עידן"
            android:theme="@style/IdanTheme"
            android:screenOrientation="portrait"/>

    </application>

</manifest>